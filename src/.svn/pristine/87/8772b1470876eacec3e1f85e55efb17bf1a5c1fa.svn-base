.pd-usertop .biao{width:35px;text-align:center;padding-left:9px;line-height:35px;color:#fff;position:absolute;top:50%;right:0;transform:translateY(-50%);-webkit-transform:translateY(-50%);background:url(../../assets/images/tzxq_bq_01@2x.png) right center no-repeat;background-size:contain;font-size:10px;}
.pd-usertop .line{border-bottom:1px solid #e4e4e4;padding-top:10px;}
.pd-word{padding:15px;}
.pd-word img{width:100%;}
.pd-eva .title{padding:0 15px;line-height:50px;border-top:1px solid #e4e4e4;border-bottom:1px solid #e4e4e4;color:#333;}
.pd-eva .pd_evaul{padding-bottom:60px;}
.pd_evaul li {padding:15px 15px 0;}
.pd_evaul li .evaTop{height:40px;position:relative;}
.pd_evaul li .evaTop .imgbox{width:40px;position:absolute;top:0;left:0;}
.pd_evaul li .evaTop .imgbox:before{content:'';display:block;padding:50% 0;}
.pd_evaul li .evaTop .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;border-radius:50%;}
.pd_evaul li .evaTop .name{padding:0 70px 0 50px;color:#666;line-height:40px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.pd_evaul li .evaTop .time{color:#999;position:absolute;top:0;right:0;line-height:40px;font-size:10px;}
.pd_evaul li .evacont{margin-left:50px;color:#333;padding-bottom:10px;border-bottom:1px solid #e4e4e4;}
.pd-evatotal{height:40px;padding:5px 0;color:#666;position:fixed;bottom:0;left:0;width:100%;z-index:2;display:-webkit-box;background:#fff;box-shadow:0 7px 30px rgba(0,0,0,.5);}
.pd-evatotal .evlf{width:50%;line-height:40px;border-right:1px solid #e4e4e4;margin-left:-1px;text-align:center;}
