.afrbtn{display:block;width:80%;height:40px;line-height:40px;text-align:center;color:#fff;background:#e54749;background:linear-gradient(to right,#ff3e4b,#c71521);background:-moz-linear-gradient(right,#ff3e4b,#c71521);background:-o-linear-gradient(right,#ff3e4b,#c71521);background:-webkit-linear-gradient(left,#ff3e4b,#c71521);position:absolute;bottom:10px;left:50%;transform:translateX(-50%);-webkit-transform:translateX(-50%);}

/*我的订单*/
.order-state-tabs {overflow: hidden;background-color: #fff;margin-bottom:10px;}
.order-state-tabs .state-btn {float: left;width: 20%;text-align: center;line-height: 40px;position: relative;color: #666;}
.order-state-tabs .state-btn.on {color: #ef3232;}
/*.yy-myorder-list{background:#fff;}*/
.yy-myorder-list li {margin-bottom: 10px;background: #fff;}
.order-tophead {color: #666666;padding: 0 70px 0 76px;line-height: 44px;position: relative;overflow: hidden;text-overflow: ellipsis;}
.order-tophead .ott-t {position: absolute;top: 0;left: 15px;}
.order-tophead .ott-sn {margin-left: 10px;}
.order-tophead .ot-tips {position: absolute;right: 15px;top: 0;color: #ef3232;}
.yy-myorder-list .y-order-pro {margin-top: 0;}
.yy-myorder-list .tt-count {background: #fff;color: #6d6d78;height: 40px;line-height: 40px;padding: 0 15px;overflow:hidden;}
.yy-myorder-list .tt-count p{float:left;}
.yy-myorder-list .tt-count span{display:block;float:right;}
.yy-myorder-list .tt-count span em{color:#ef3232;font-size:16px;}
.yy-myorder-list .btbox {float: right;padding: 8px 15px 8px 0;background: #fff;}
.yy-myorder-list .btbox a {color:#666;background:#eeeeee;display: block;height: 30px;width: 85px;float: left;box-sizing: border-box;line-height: 30px;text-align: center;margin-left: 12px;}
.y_btnbox{overflow:hidden;border-top:1px solid #e4e4e4;}
.y-order-pro .y_name_t span{font-size:12px;float:left;}
.o_btnbox{overflow:hidden;border-top:1px solid #e4e4e4;position:fixed;bottom:0;left:0;width:100%;background:#fff;}
.o_btnbox .btbox{float:right;padding:8px 15px 8px 0;background:#fff;}
.o_btnbox .btbox a {border: 1px solid #ef3232;color: #ef3232;display: block;height: 30px;width: 85px;float: left;box-sizing: border-box;line-height: 30px;text-align: center;border-radius: 5px;margin-left: 12px;}
.o_btnbox .btbox a.grey{color:#666;border:1px solid #333}
.my_order_wrap.pd{padding-bottom:50px;}
.y-order-pro{margin-top:10px;background:#fff;}
.y-order-pro .y-kein {display: block;position: relative;overflow: hidden;min-height: 80px;border-bottom: 1px solid #f0f0f0;padding: 10px 15px 10px 95px;}
.y-order-pro .y-imgbox {left: 15px;width: 70px;height: 70px;position: absolute;top: 10px;}
.y-order-pro .y-imgbox .y-img {width: 100%;height: 100%;position: relative;}
.y-order-pro .y-imgbox .y-img .ab {position: absolute;left: 0;top: 0;right: 0;bottom: 0;}
.y-order-pro .y-img img {width: 100%;height:100%;}

/* post add */
.g-core-image-upload-btn{opacity: 0;}

/*评价*/
.evagoods{display:-webkit-box;padding:15px;background:#fff;}
.evagoods .imgbox{width:23%;position:relative;}
.evagoods .imgbox:before{content:'';display:block;padding:50% 0;}
.evagoods .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;}
.evagoods .evastar{width:71%;margin-left:6%;}
.evagoods .evastar p{color:#333;font-size:16px;}
.estar .estar_cont .star {margin-top:10px;margin-right:10px;display: inline-block;width: 22px;height: 22px;background: url(../../assets/images/white_star.png) center center no-repeat;background-size: contain;vertical-align:middle;}
.estar .estar_cont .star.on {background: url(../../assets/images/violet_star.png) center center no-repeat;background-size: contain;}
/*评价成功*/
.evasuccess img{display:block;width:35px;height:35px;margin:25px auto 15px;}
.evasuccess p{text-align:center;color:#666;}