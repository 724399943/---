.pd-evatotal .evrg .rg .collect.on{background:url(../../assets/images/sy_sc_on_icn@2x.png) center center no-repeat;background-size:35px;}
.pd-evatotal .evrg .rg .comment{background:url(../../assets/images/sy_pl_icn@2x.png) center center no-repeat;background-size:35px;}

/*发帖*/
.postWarp{position:absolute;top:40px;left:0;right:0;bottom:0;overflow:auto;background:#fff;}
.pcontent{padding:0 15px;}
.pcontent .ptop{border-bottom:1px solid #e4e4e4;padding:10px 0;}
.pcontent .ptop .tinput{width:100%;border:none;color:#666;box-sizing:border-box;-webkit-tap-highlight-color: rgba(0,0,0,0);-webkit-user-modify: read-write-plaintext-only;outline: 0;word-wrap: break-word;word-break: break-all;}
.p-postmsg{padding:15px 0 70px;}
.p-postmsg .texa{width:100%;min-height:40px;color:#666;border:none;box-sizing:border-box;-webkit-tap-highlight-color: rgba(0,0,0,0);-webkit-user-modify: read-write-plaintext-only;outline: 0;word-wrap: break-word;word-break: break-all;}
.p-postmsg .texa .imgbox{width:40%;position:relative;}
.p-postmsg .texa .imgbox:before{content:'';display:block;padding:30% 0;}
.p-postmsg .texa .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;}
.p-postmsg .texa .imgbox .updata{display:block;width:40%;background:url(../../assets/images/fbtz_sc_icn@2x.png) center center no-repeat;background-size:contain;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}
.p-postmsg .texa .imgbox .updata:before{content:'';display:block;padding:50% 0;}
.p-postmsg .texa .imgbox .btn{width:70%;position:absolute;top:50%;left:50%;transform:translate(-50%,-50%);-webkit-transform:translate(-50%,-50%);}
.p-postmsg .texa .imgbox .btn span{display:block;text-align:center;border-radius:5px;line-height:30px;color:#fff;background:#a1a1a1;position: relative;}
.p-postmsg .texa .imgbox .btn span.red{background:#fd3c49;}
.p-postmsg .texa .imgbox .btn span.red input{position:absolute;top:0;left:0;opacity:0;width:100%;height:100%;}
.p-fun {position:fixed;bottom:0;left:0;width:100%;height:40px;overflow:hidden;background:#fff;padding:5px 3%;border-top:1px solid #e4e4e4;z-index: 1501}
.p-fun .p-ftext{background:#eee;width:55%;height:40px;float:left;}
.p-fun .p-uploadimg{width:15%;height:40px;margin:0 2%;background:url(../../assets/images/fbtz_zp_button@2x.png) center center no-repeat;background-size:cover;float:left;}
.p-fun .p-uploadimg input{opacity:0;width:100%;height:100%;}
.p-fun .submit{display:block;width:20%;color:#fff;line-height:40px;background:#e54749;background:linear-gradient(to right,#ff3e4b,#c71521);background:-moz-linear-gradient(right,#ff3e4b,#c71521);background:-o-linear-gradient(right,#ff3e4b,#c71521);background:-webkit-linear-gradient(left,#ff3e4b,#c71521);text-align:center;float:left;}

/*体验馆*/
.expul{overflow:hidden;}
.expul li{padding:15px;background:#fff;position:relative;margin-bottom:10px;display:-webkit-box;transition:all 0.2s;}
.expul li .imgbox{width:24%;position:relative;}
