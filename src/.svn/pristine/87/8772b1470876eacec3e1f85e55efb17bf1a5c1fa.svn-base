/*头部导航*/
.head {height: 40px;line-height: 40px;position: fixed;top: 0px;left: 0;width: 100%;background: #fff;z-index:9;border-bottom:1px solid #e4e4e4;}
.head .back {height: 100%;width: 55px;background: url(../../assets/images/nav_back_icn@2x.png) no-repeat 30% center;background-size: 80%;display: block;position: absolute;top: 0px;left: 0;font-size:0.8rem;text-align:right;}
.y-confirm-order-h1 {display: block;height: 100%;text-align: center;font-size: 18px;color: #333;padding:0 60px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.head .mail{line-height:40px;text-align:center;color:#999;position:absolute;top:0;right:10px;}
.head .topost {padding: 0 13px;height: 100%;width: 30px;background: url(../../assets/images/nav_fx_icn@2x.png) no-repeat center center;background-size: 35px;display: block;position: absolute;right: 0;top: 0;}
.head .close {height: 100%;width: 55px;background: url(../../assets/images/login_del_icn@2x.png) no-repeat 30% center;background-size: 80%;display: block;position: absolute;top: 0px;left: 0;font-size:0.8rem;text-align:right;}
.head .search {padding: 0 13px;height: 100%;width: 30px;background: url(../../assets/images/nav_search_icn@2x.png) no-repeat center center;background-size: 35px;display: block;position: absolute;right: 0;top: 0;}
.head .collect {padding: 0 13px;height: 100%;width: 30px;background: url(../../assets/images/sd_xin_ico.png) no-repeat center center;background-size: 35px;display: block;position: absolute;right: 0;top: 0;}
.head .collect.on{background: url(../../assets/images/sd_redxin_ico.png) no-repeat center center;background-size: 35px;}
.head .text {padding: 0 13px;height: 100%;width: 30px;display: block;position: absolute;right: 0;top: 0;color:#666;}
.head .close {height: 100%;width: 55px;background: url(../../assets/images/login_del_icn@2x.png) no-repeat 30% center;background-size: 80%;display: block;position: absolute;top: 0px;left: 0;font-size:0.8rem;text-align:right;}

.main{padding-top:40px;font-size:15px;}
.mask{position:fixed;top:0;left:0;width:100%;height:100%;background:rgba(0,0,0,.4);z-index:10;}

/*底部固定菜单*/
.y-ycar-foot {height: 48px;background: #fff;bottom: 0;position: fixed;left: 0;right: 0;border-top:1px solid #e4e4e4;}
.y-ycar-foot a {width: 20%;display: block;height: 100%;float: left;position:relative;}
.y-ycar-foot a .icon {display: block;padding-top: 30px;height: 20px;line-height: 20px;color: #817d7d;font-size: 12px;text-align: center;position: relative;}
.y-ycar-foot .icon i {position: absolute;top: 8px;left: 50%;right: 0;display: block;width: 22px;height: 22px;margin-left: -11px;}
.y-ycar-foot a:nth-child(1) i {background: url(../images/tabbar_home_icn@2x.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(2) i {background: url(../images/tabbar_fl_icn@2x.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(3) i {background: url(../images/tabbar_circle_icn@2x.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(4) i {background: url(../images/tabbar_gwc_icn@2x.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a:nth-child(5) i {background: url(../images/tabbar_wd_icn@2x.png) center center no-repeat;background-size: contain;}
.y-ycar-foot a.on .icon {color: #e54749;}
.y-ycar-foot a.on:nth-child(1) i {background:url(../images/tabbar_home_icn_sel@2x.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(2) i {background:url(../images/tabbar_fl_icn_sel@2x.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(3) i {background:url(../images/tabbar_circle_icn_sel@2x.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(4) i {background:url(../images/tabbar_gwc_icn_sel@2x.png) center center no-repeat;background-size:contain;}
.y-ycar-foot a.on:nth-child(5) i {background:url(../images/tabbar_wd_icn_sel@2x.png) center center no-repeat;background-size:contain;}

/*过渡动画*/
.vux-pop-out-enter-active,.vux-pop-out-leave-active,.vux-pop-in-enter-active,.vux-pop-in-leave-active {will-change: transform;transition: all 300ms;width: 100%;height:100%;top: 0px;position: absolute;backface-visibility: hidden;perspective: 1000;}
.vux-pop-out-enter {opacity: 0;transform: translate3d(-100%, 0, 0);}
.vux-pop-out-leave-active {opacity: 0;transform: translate3d(100%, 0, 0);}
.vux-pop-in-enter {opacity: 0;transform: translate3d(100%, 0, 0);}
.vux-pop-in-leave-active {opacity: 0;transform: translate3d(-100%, 0, 0);}

/*checkbox*/
.check_box{width:20px;height:20px;margin:8px 4px;position:relative;}
.check_box input{width:21px;height:21px;visibility:hidden;position:absolute;top:0;left:0;}
.check_box label{width:22px;height:22px;display:inline-block;position:absolute;top:0;left:0;background:url(../../assets/images/qg_xz_mrbox@2x.png) no-repeat;background-size:100% 100%;}
.check_box input[type="checkbox"]:checked+label{width:22px;height:22px;background:url(../../assets/images/qg_xz_box@2x.png) no-repeat;background-size:100% 100%;}

/*登录*/
.head .mail.la{font-size:15px;color:#333;}
.login_wrap {padding: 0 15px;box-sizing: border-box;overflow: hidden;background-color:#fff;}
.logoBox {width: 20%;position: relative;margin: 15% auto;}
.logoBox:before {content: '';display: block;padding: 50% 0;}
.logoBox .logo {width: 100%;height: 100%;position: absolute;top: 0;left: 0;}
.logoBox .name {width: 100%;position: absolute;bottom: -30%;left: 0;transform: scale3d(1,1.5,1);-webkit-transform: scale3d(1,1.5,1);}
.login_wrap.all{position:absolute;top:0;left:0;right:0;bottom:0;overflow:auto;width:100%;}
.login_wrap .close{width:30px;height:30px;display:block;position:fixed;top:16px;right:16px;background:url(../../assets/images/login_del_icn@2x.png) center center no-repeat;background-size:cover;}
.login_input {width:80%;height: 50px;margin:0 auto;border-bottom: 1px solid #dfe0ea;position: relative;}
.login_input input {height: 50px;line-height: 50px;width: 100%;border: none;color:#666;}
.login_input em{display:block;width:30px;height:100%;background:url(../../assets/images/ogin_yc_icn@2x.png) center center no-repeat;background-size:contain;position:absolute;top:0;right:0;}
.login_input em.on{background:url(../../assets/images/ogin_xs_icn@2x.png) center center no-repeat;background-size:contain;}
.linput {width:80%;margin:0 auto;height:50px;line-height:50px;overflow:hidden;color:#999;}
.linput .lset{float:left;}
.linput .lpass{float:right;}
.login_btn {width: 80%;height: 45px;background: #e54749;background:linear-gradient(to right,#ff3e4b,#c71521);background:-moz-linear-gradient(right,#ff3e4b,#c71521);background:-o-linear-gradient(right,#ff3e4b,#c71521);background:-webkit-linear-gradient(left,#ff3e4b,#c71521);color: #fff;display: block;text-align: center;line-height: 45px;margin: 27px auto 0;border-radius: 5px;font-size: 16px;}
.cooperative-login{position:fixed;bottom:0;left:0;width:100%;padding:15px 0;}
.cooperative-login .box {overflow: hidden;text-align:center;}
.cooperative-login .box .l-g-b {display:inline-block;margin:0 8%;}
.cooperative-login .box .l-g-b img{width:35px;height:35px;}
.cooperative-login .box .l-g-b p {text-align: center;font-size: 12px;color: #999;padding: 3px 0;}
.loginTips{line-height:40px;text-align:center;margin:3% auto 0;display: none;}
.loginTips span{background:#bbb;color:#fff;border-radius:5px;padding:10px 15px;}

/*注册*/
.enr_input{background:#fff;position:relative;}
.enr_input.all{position:absolute;top:0;left:0;bottom:0;right:0;overflow:auto;width:100%;}
.enr_input .close{width:30px;height:30px;display:block;position:fixed;top:16px;right:16px;background:url(../../assets/images/login_del_icn@2x.png) center center no-repeat;background-size:cover;}
.enr_input ul{padding:0 15px;width:80%;margin:0 auto;}
.enr_input ul .line{border-bottom:1px solid #e4e4e4;}
.enr_input ul li{line-height:35px;color:#333;}
.enr_input ul li .einput{overflow:hidden;height:50px;}
.enr_input ul li .einput.pmr{padding-right:30px;position:relative;}
.enr_input ul li .einput.pmr em{display:block;width:30px;height:100%;background:url(../../assets/images/ogin_yc_icn@2x.png) center center no-repeat;background-size:contain;position:absolute;top:0;right:0;}
.enr_input ul li .einput.pmr em.on{background:url(../../assets/images/ogin_xs_icn@2x.png) center center no-repeat;background-size:contain;}
.enr_input ul li input{border:none;width:100%;height:50px;line-height:50px;}
.enr_input ul li .einput .tips{text-align:center;color:#999;line-height:50px;}
.enr_input ul li .einput .code{margin-left:0px;width:50%;float:left;line-height:35px;}
.enr_input ul li .einput .getCode_btn{float:right;color:#c71521;border:none;line-height:30px;background:#fff;border-left:1px solid #e4e4e4;padding:0 3%;margin:10px 0;}
.enr_input ul li .einput .res{color:#999;display:inline-block;}
.enr_input ul li .einput .rin{width:60%;}
.enr_input ul li .einput .eag{color:#e54749;display:inline-block;}
.enr_input .ccust{text-align:center;color:#333;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;margin:10% 0;}
.enr_input .register_btn{display:block;font-size:16px;line-height:45px;color:#fff;background:#e54749;background:linear-gradient(to right,#ff3e4b,#c71521);background:-moz-linear-gradient(right,#ff3e4b,#c71521);background:-o-linear-gradient(right,#ff3e4b,#c71521);background:-webkit-linear-gradient(left,#ff3e4b,#c71521);text-align:center;width:80%;margin:17% auto 0;border-radius:5px;}
.agreementWrap{background:#fff;}

/*忘记密码*/
.forgetTop{padding:15% 0;}
.forgetTop .logobox{width:30%;margin:0 auto;}
.forgetTop .logobox img{width:100%;}
.forgetTop h1{color:gray;text-align:center;font-size:18px;font-weight:bold;line-height:40px;}
.forgetTop .tips{text-align:center;color:#999;}

/*装修圈*/
.decCircle{padding-bottom:60px;}
.decCircle.pd{padding-bottom:0px;}
.decCircle .decNav{background:#fff;border-bottom:1px solid #e4e4e4;height:40px;line-height:40px;overflow:hidden;position:fixed;top:40px;left:0;width:100%;z-index:2;}
.decCircle .decNav span{display:block;width:33.3%;text-align:center;color:#333;float:left;position:relative;}
.decCircle .decNav span.on{color:#cc1925;}
.decCircle .decNav span em{position:absolute;bottom:3px;left:50%;transform:translateX(-50%);-webkit-transform:translateX(-50%);width:7%;height:3px;background:#cc1925;border-radius:35%;display:none;}
.decCircle .decNav span.on em{display:block;}
.decCircle .decContent{padding-top:40px;}
.decCircle .decContent .decCont{margin-top:15px;background:#fff;padding:15px;color:#666;position:relative;}
.decCont .sign{width:35px;height:20px;background:url(../../assets/images/zxq_jpbg_01@2x.png) left center no-repeat;background-size:cover;text-align:center;line-height:20px;color:#fff;font-size:10px;padding-left:8px;position:absolute;top:-7px;left:10px;}
.decCont .dectop{position:relative;}
.decCont .dectop h1{color:#333;padding-right:90px;}
.decCont .dectop span{position:absolute;top:0;right:5px;}
.decCont .txt{padding:5px 0;overflow: hidden; text-overflow: ellipsis;-webkit-line-clamp: 3;-webkit-box-orient: vertical; display: -webkit-box;}
.decCont .decUser{position:relative;height:40px;}
.decCont .decUser .imgbox{width:40px;height:40px;position:absolute;top:50%;left:0;transform:translateY(-50%);-webkit-transform:translateY(-50%);}
.decCont .decUser .imgbox:before{content:'';display:block;padding:50% 0;}
.decCont .decUser .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;border-radius:50%;}
.decCont .decUser .d-uname{padding:0 70px 0 50px;color:#666;position:absolute;top:50%;left:0;transform:translateY(-50%);-webkit-transform:translateY(-50%);}
.decCont .decUser .d-uname p{overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
.decCont .decUser .d-uname span{display:block;font-size:10px;}
.decCont .decUser .d-see{height:40px;line-height:40px;position:absolute;top:50%;right:0;transform:translateY(-50%);-webkit-transform:translateY(-50%);}
.decCont .decUser .d-see em{width:23px;height:23px;display:inline-block;background:url(../../assets/images/zxq_pl_icn@2x.png) center center no-repeat;background-size:cover;vertical-align:middle;}
.dec-posting{width:23%;position:fixed;bottom:50px;right:15px;display:block;z-index:2;}
.dec-posting:before{content:'';display:block;padding:50% 0;}
.dec-posting img{width:100%;height:100%;position:absolute;top:0;left:0;}

/*帖子详情*/
.postDetail{background-color:#fff;padding-bottom:50px;}
.pd-usertop {height:40px;position:relative;padding:15px;}
.pd-usertop .imgbox{width:40px;position:absolute;top:50%;left:10px;transform:translateY(-50%);-webkit-transform:translateY(-50%);}
.pd-usertop .imgbox:before{content:'';display:block;padding:50% 0;}
.pd-usertop .imgbox img{width:100%;height:100%;position:absolute;top:0;left:0;border-radius:50%;}
.pd-usertop .pd-name{padding:0 45px;color:#666;line-height:40px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap;}
