.toTop img{width:100%;height:100%;position:absolute;top:0;left:0;}
.expul li .coll-a{display:-webkit-box;width:100%;}

/*搜索*/
.search_cate {height: 30px;margin: 5px 0 5px;padding:0 60px;overflow: hidden;border-radius: 4px;line-height:30px;}
.search_cate input {width: 100%;height: 100%;background:#f2f2f2;border: none;border-radius: 4px;float: left;color:#666;text-indent:1em;}
.search_cate input::-webkit-input-placeholder {color:#666;}
.search_cate input:-moz-placeholder {color:#666;}
.search_cate input::-moz-placeholder {color:#666;}
.search_cate input:-ms-input-placeholder {color:#666;}
.noResult .imgbox{width:60%;position:relative;margin:20% auto 0;}
.noResult .imgbox img{width:100%;height:100%;}
.noResult p{text-align:center;color:#666;line-height:50px;}

/*限时抢购*/
.flashSale-m .fs-top{height:40px;line-height:40px;background:#2f2f2f;color:#fff;padding-left:50px;position:relative;}
.flashSale-m .fs-top em{display:block;width:20px;height:20px;background:url(../../assets/images/qglb_time_icn.png) center center no-repeat;background-size:20px;position:absolute;top:50%;left:15px;transform:translateY(-50%);-webkit-transform:translateY(-50%);}
.expul .pp-msg .p-ptcont {overflow:hidden;}
.expul .pp-msg .p-ptcont .price{float:left;padding-top:10px;}
.expul .pp-msg .p-ptcont .price span{display:inline-block;color:#666;font-size:16px;}
.expul .pp-msg .p-ptcont .price .old{text-decoration:line-through;color:#999;font-size:10px;}
.expul .pp-msg .p-ptcont .price .red{color:#ff3e4b;}
